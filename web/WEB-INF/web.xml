<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MatchAPIController</servlet-name>
        <servlet-class>Controller.API.Match.MatchAPIController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServerAPIController</servlet-name>
        <servlet-class>Controller.ServerAPI.ServerAPIController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateMatch</servlet-name>
        <servlet-class>Controller.API.Match.CreateMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterMatch</servlet-name>
        <servlet-class>Controller.API.Match.RegisterMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllActiveServerAPI</servlet-name>
        <servlet-class>Controller.ServerAPI.GetAllActiveServerAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditServerAPI</servlet-name>
        <servlet-class>Controller.ServerAPI.EditServerAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.Login.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainDashboardController</servlet-name>
        <servlet-class>Controller.Dashboard.MainDashboardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewServerListController</servlet-name>
        <servlet-class>Controller.Dashboard.ViewServerListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MatchAPIController</servlet-name>
        <url-pattern>/api/match</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerAPIController</servlet-name>
        <url-pattern>/api/servers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateMatch</servlet-name>
        <url-pattern>/api/match/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterMatch</servlet-name>
        <url-pattern>/api/match/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllActiveServerAPI</servlet-name>
        <url-pattern>/api/server/getactiveservers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditServerAPI</servlet-name>
        <url-pattern>/api/server/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainDashboardController</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewServerListController</servlet-name>
        <url-pattern>/server/list</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
